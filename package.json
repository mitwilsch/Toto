{
  "name": "toto",
  "version": "0.0.1",
  "description": "toto: todo app",
  "main": "src/server/index.js",
  "scripts": {
    "build": "concurrently \"docker build -f ./Dockerfile-client -t mitwilsch/toto/client .\" \"docker build -f ./Dockerfile-server -t mitwilsch/toto/server .\"",
    "devClient": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "devServer": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run devServer\" \"npm run devClient\"",
    "prodClient": "webpack-dev-server --mode production",
    "prodServer": "node src/server/index.js",
    "prod": "npm run build && concurrently \"npm run prodClient\" \"npm run prodServer\"",
    "db": "docker run mongodb",
    "deploy": "docker-compose up"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "auth0-js": "^9.12.2",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "history": "^4.10.1",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.9.5",
    "react-router-dom": "^5.1.2",
    "webpack-dev-server": "^3.10.0",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint_d": "^8.0.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  }
}
